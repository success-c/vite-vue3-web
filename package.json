{
  "name": "future-study",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start:dev": "vite --mode dev",
    "start:prod": "vite --mode prod",
    "build:dev": " vite build --mode dev",
    "build:prod": " vite build --mode prod",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "git-cz",
    "stylelint": "stylelint src/**/*.{css,less,scss,vue} --fix",
    "lint": "eslint src --ext .vue,.js,.ts,.jsx,.tsx  --fix",
    "format": "prettier --write \"src/**/*.{html,vue,ts,js,json,md}\"",
    "preinstall": "npx only-allow pnpm"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@arco-design/web-vue": "^2.54.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "pinia": "^2.1.7",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0",
    "vue-tsc": "^1.8.27"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "axios": "^1.6.7",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.22.0",
    "husky": "^9.0.11",

    "prettier": "^3.2.5",
    "rollup-plugin-external-globals": "^0.9.2",
    "sass": "^1.71.1",
    
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-vue-setup-extend-plus": "^0.1.0"
  }
}
